<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd
        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

    <changeSet author="Boruch" id="changelog_024_register">
        <createTable tableName="register">
            <column name="id" type="bigint" />
            <column name="classes_type" type="varchar(255)">
                <constraints nullable="true"/>
            </column>
            <column name="activity_start" type="timestamp">
                <!--<constraints nullable="false"/>-->
            </column>
            <column name="leader_id" type="bigint">
                <constraints nullable="false"/>
            </column>
        </createTable>

        <addPrimaryKey
                columnNames="id"
                constraintName="pk_register"
                tableName="register"/>
        <createSequence
                sequenceName="register_seq"
                startValue="1"
                incrementBy="1"/>

    </changeSet>

</databaseChangeLog>